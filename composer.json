{
  "name": "csrdelft/bb",
  "description": "C.S.R. BB Parser",
  "type": "library",
  "homepage": "https://csrdelft.github.io/bb",
  "license": "MIT",
  "authors": [
    {
      "name": "PubCie der C.S.R. Delft",
      "email": "pubcie@csrdelft.nl"
    }
  ],
  "require": {
    "php": ">=7.3.19",
    "symfony/dependency-injection": "^5.2",
    "symfony/http-kernel": "^5.2",
    "symfony/config": "^5.2"
  },
  "autoload": {
    "psr-4": {
      "CsrDelft\\BbBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "CsrDelft\\BbBundle\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "@php vendor/phpunit/phpunit/phpunit tests"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.3",
    "spatie/phpunit-snapshot-assertions": "^2.1",
    "symfony/phpunit-bridge": "^5.2"
  }
}
